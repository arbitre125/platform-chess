{
  "version": 1,
  "paths": {
    "/player/search/{name}": {
      "name": "/player/search/{name}",
      "lambdaFunction": "searchECFPlayer",
      "permissions": {
        "setting": "protected",
        "auth": [
          "read"
        ],
        "guest": [
          "read"
        ]
      }
    },
    "/player/update": {
      "name": "/player/update",
      "lambdaFunction": "memberECFUpdate",
      "permissions": {
        "setting": "private",
        "auth": [
          "update"
        ]
      }
    }
  }
}